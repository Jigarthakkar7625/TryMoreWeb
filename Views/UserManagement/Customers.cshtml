@model List<TryMoreWeb.Models.UsersModel>
@{
    ViewBag.Title = "Customers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




<!-- ============================================================== -->
<!-- main wrapper -->
<!-- ============================================================== -->
<!-- ============================================================== -->
<!-- navbar -->
<!-- ============================================================== -->
<!-- ============================================================== -->
<!-- end navbar -->
<!-- ============================================================== -->
<!-- ============================================================== -->
<!-- left sidebar -->
<!-- ============================================================== -->
<!-- ============================================================== -->
<!-- end left sidebar -->
<!-- ============================================================== -->
